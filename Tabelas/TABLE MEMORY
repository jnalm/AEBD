CREATE TABLE MEMORY 
(
  IDMEMORY NUMBER NOT NULL 
, STATISTIC VARCHAR2(200 BYTE) 
, SPAPOOL VARCHAR2(200 BYTE) 
, BYTES VARCHAR2(200 BYTE) 
, CONSTRAINT TABLE1_PK PRIMARY KEY 
  (
    IDMEMORY 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TABLE1_PK ON MEMORY (IDMEMORY ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
