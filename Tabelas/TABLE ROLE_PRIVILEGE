CREATE TABLE ROLE_PRIVILEGE 
(
  ID_ROLE_PRIVILEGE NUMBER NOT NULL 
, ID_ROLE NUMBER 
, ID_PRIVILEGE NUMBER 
, CONSTRAINT ROLE_PRIVILEGE_PK PRIMARY KEY 
  (
    ID_ROLE_PRIVILEGE 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX ROLE_PRIVILEGE_PK ON ROLE_PRIVILEGE (ID_ROLE_PRIVILEGE ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE ROLE_PRIVILEGE
ADD CONSTRAINT ROLE_PRIVILEGE_FK1 FOREIGN KEY
(
  ID_ROLE 
)
REFERENCES ROLE
(
  IDROLE 
)
ENABLE;

ALTER TABLE ROLE_PRIVILEGE
ADD CONSTRAINT ROLE_PRIVILEGE_FK2 FOREIGN KEY
(
  ID_PRIVILEGE 
)
REFERENCES PRIVILEGE
(
  IDPRIVILEGE 
)
ENABLE;