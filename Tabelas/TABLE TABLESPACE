CREATE TABLE TABLESPACE 
(
  ID_TABLESPACE NUMBER NOT NULL 
, MAXIMUMSIZE VARCHAR2(20 BYTE) 
, TS_SIZE VARCHAR2(20 BYTE) 
, NAME VARCHAR2(200 BYTE) 
, TYPE VARCHAR2(200 BYTE) 
, AUTOEXTEND VARCHAR2(20 BYTE) 
, FREESPACE VARCHAR2(20 BYTE) 
, CONSTRAINT TABLESPACE_PK PRIMARY KEY 
  (
    ID_TABLESPACE 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TABLESPACE_PK ON TABLESPACE (ID_TABLESPACE ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;