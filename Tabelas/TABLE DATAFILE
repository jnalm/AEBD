CREATE TABLE DATAFILE 
(
  ID_DATAFILE NUMBER NOT NULL 
, FREESPACE VARCHAR2(20 BYTE) 
, AUTOEXTEND VARCHAR2(20 BYTE) 
, NAME VARCHAR2(20 BYTE) 
, TYPE VARCHAR2(20 BYTE) 
, MAXIMUMSIZE VARCHAR2(20 BYTE) 
, DF_SIZE VARCHAR2(20 BYTE) 
, ID_TABLESPACE NUMBER 
, CONSTRAINT DATAFILE_PK PRIMARY KEY 
  (
    ID_DATAFILE 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX DATAFILE_PK ON DATAFILE (ID_DATAFILE ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE DATAFILE
ADD CONSTRAINT DATAFILE_FK1 FOREIGN KEY
(
  ID_TABLESPACE 
)
REFERENCES TABLESPACE
(
  ID_TABLESPACE 
)
ENABLE;