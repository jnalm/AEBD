CREATE TABLE ROLE 
(
  IDROLE NUMBER NOT NULL 
, IMPLICIT VARCHAR2(200 BYTE) 
, ROLE VARCHAR2(200 BYTE) 
, INHERITED VARCHAR2(200 BYTE) 
, PASSWORD_REQUIRED VARCHAR2(200 BYTE) 
, COMMON VARCHAR2(200 BYTE) 
, ORACLE_MAINTAINED VARCHAR2(200 BYTE) 
, AUTHENTICATION_TYPE VARCHAR2(200 BYTE) 
, CONSTRAINT ROLE_PK PRIMARY KEY 
  (
    IDROLE 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX ROLE_PK ON ROLE (IDROLE ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;