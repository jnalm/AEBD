CREATE TABLE SESSIONDB 
(
  IDSESSION NUMBER NOT NULL 
, RESOURCE_CONSUMER_GROUP VARCHAR2(200 BYTE) 
, COMMAND VARCHAR2(200 BYTE) 
, MODULE VARCHAR2(200 BYTE) 
, ACTION VARCHAR2(200 BYTE) 
, MACHINE VARCHAR2(200 BYTE) 
, OSUSER VARCHAR2(200 BYTE) 
, STATUS VARCHAR2(200 BYTE) 
, SQL_ID VARCHAR2(200 BYTE) 
, SQL_CHILD_NUMBER VARCHAR2(200 BYTE) 
, INSTID VARCHAR2(200 BYTE) 
, SERIAL VARCHAR2(200 BYTE) 
, CLIENT_INFO VARCHAR2(200 BYTE) 
, SID VARCHAR2(200 BYTE) 
, SECS_IN_WAIT VARCHAR2(200 BYTE) 
, USER_IDUSER NUMBER 
, CONSTRAINT ID_SESSION PRIMARY KEY 
  (
    IDSESSION 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TABLE1_PK1 ON SESSIONDB (IDSESSION ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE SESSIONDB
ADD CONSTRAINT ID_FK_SESSION FOREIGN KEY
(
  USER_IDUSER 
)
REFERENCES DBUSER
(
  ID_USER 
)
ENABLE;