CREATE TABLE TABLESPACE_USER 
(
  ID_TABLESPACE_USER NUMBER NOT NULL 
, ID_TABLESPACE NUMBER 
, ID_USER NUMBER 
, CONSTRAINT TABLESPACE_USER_PK PRIMARY KEY 
  (
    ID_TABLESPACE_USER 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TABLESPACE_USER_PK ON TABLESPACE_USER (ID_TABLESPACE_USER ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE TABLESPACE_USER
ADD CONSTRAINT TABLESPACE_USER_FK1 FOREIGN KEY
(
  ID_TABLESPACE 
)
REFERENCES TABLESPACE
(
  ID_TABLESPACE 
)
ENABLE;

ALTER TABLE TABLESPACE_USER
ADD CONSTRAINT TABLESPACE_USER_FK2 FOREIGN KEY
(
  ID_USER 
)
REFERENCES DBUSER
(
  ID_USER 
)
ENABLE;