CREATE TABLE DBUSER 
(
  ID_USER NUMBER NOT NULL 
, NAME VARCHAR2(200 BYTE) 
, EXPIRATIONDATE VARCHAR2(200 BYTE) 
, CREATIONDATE VARCHAR2(200 BYTE) 
, COMMON VARCHAR2(200 BYTE) 
, ACCOUNTSTATUS VARCHAR2(200 BYTE) 
, PROFILE VARCHAR2(200 BYTE) 
, CONSTRAINT ID_USER PRIMARY KEY 
  (
    ID_USER 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX ID_USER ON DBUSER (ID_USER ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;