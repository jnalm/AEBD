CREATE TABLE CPU 
(
  IDCPU NUMBER NOT NULL 
, DBID VARCHAR2(200 BYTE) 
, CPU_CORE_COUNT VARCHAR2(200 BYTE) 
, CPU_COUNT VARCHAR2(200 BYTE) 
, TIMESTAMP VARCHAR2(200 BYTE) 
, VERSION VARCHAR2(200 BYTE) 
, CPU_SOCKET_COUNT VARCHAR2(200 BYTE) 
, CONSTRAINT CPU_PK PRIMARY KEY 
  (
    IDCPU 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX CPU_PK ON CPU (IDCPU ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
