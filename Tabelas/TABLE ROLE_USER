CREATE TABLE ROLE_USER 
(
  ID_ROLE_USER NUMBER NOT NULL 
, ID_USER NUMBER NOT NULL 
, ID_ROLE NUMBER 
, CONSTRAINT ROLE_USER_PK PRIMARY KEY 
  (
    ID_ROLE_USER 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX ROLE_USER_PK ON ROLE_USER (ID_ROLE_USER ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
