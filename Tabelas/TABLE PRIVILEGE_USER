CREATE TABLE PRIVILEGE_USER 
(
  ID_PRIVILEGE_USER NUMBER NOT NULL 
, ID_PRIVILEGE NUMBER 
, ID_USER NUMBER 
, CONSTRAINT PRIVILEGE_USER_PK PRIMARY KEY 
  (
    ID_PRIVILEGE_USER 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX PRIVILEGE_USER_PK ON PRIVILEGE_USER (ID_PRIVILEGE_USER ASC) 
      LOGGING 
      TABLESPACE AEBD_PROJECT 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE AEBD_PROJECT 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE PRIVILEGE_USER
ADD CONSTRAINT PRIVILEGE_USER_FK1 FOREIGN KEY
(
  ID_PRIVILEGE 
)
REFERENCES PRIVILEGE
(
  IDPRIVILEGE 
)
ENABLE;

ALTER TABLE PRIVILEGE_USER
ADD CONSTRAINT PRIVILEGE_USER_FK2 FOREIGN KEY
(
  ID_USER 
)
REFERENCES DBUSER
(
  ID_USER 
)
ENABLE;